{
    "name": "magicbox/laraquickkit",
    "description": "A Laravel package to simplify web development by offering various modules like CRM, Inventory, and HR.",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "LaraQuickKit",
        "email": "recodeui@gmail.com"
      }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^10.0",
        "spatie/laravel-permission": "^5.0"
    },
    "autoload": {
        "psr-4": {
        "MagicBox\\LaraQuickKit\\": "src/"
        }
    },
    "extra": {
        "laravel": {
        "providers": [
            "MagicBox\\LaraQuickKit\\Providers\\LaraQuickKitServiceProvider"
        ]
        }
    },
    "scripts": {
        "post-autoload-dump": []
    },
    "minimum-stability": "stable",
    "prefer-stable": true
  }
  