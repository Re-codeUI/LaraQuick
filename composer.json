{
    "name" : "magicbox/laraquick-kit",
    "description": "Fast Laravel Application Generator.",
    "keywords": ["laravel","package","quick","application generator"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Re-codeUI",
            "email": "recodeui@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Magicbox\\LaraQuickKit\\": "src/"
        }
    },
    "require":{
        "php": "^8.0",
        "illuminate/support": "^10.0 || ^11.0",
        "spatie/laravel-permission": "^6.10"
    },
    "extra":{
        "laravel": {
            "providers": [
                "Magicbox\\LaraQuickKit\\Providers\\LaraQuickServiceProvider"
            ],
            "aliases": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
